CREATE TABLE d_tweets (
  id bigint PRIMARY KEY,
  text text,

  user_screen_name text,
  user_id bigint,
  user_url text,

  tweet_timestamp_ms timestamp
);

CREATE INDEX ON d_tweets (user_id);

CREATE TABLE n_tweets (
  id bigint PRIMARY KEY,
  text text,

  user_screen_name text,
  user_id bigint,
  user_url text,

  tweet_timestamp_ms timestamp
);

CREATE INDEX ON n_tweets (user_id);
